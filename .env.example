# Required (no default): must match OpenWebUI's EXTERNAL_DOCUMENT_LOADER_API_KEY
ENGINE_API_KEY=supersecret

# Default: all (or set ranges like "1-5")
PDF_PAGES=all

# Default: 6000
MAX_DOC_CHARS=6000
# Default: 800
OVERLAP_CHARS=800
# Default: 200
MAX_TEXT_PAGES=200

# Default: 4
MAX_HEADER_ROWS=4

# Default: 40
CAMELOT_LATTICE_LINE_SCALE=40
# Default: 200
CAMELOT_STREAM_EDGE_TOL=200
# Default: 10
CAMELOT_STREAM_ROW_TOL=10

# Default: 2
MIN_ROWS_FOR_TABLE=2
# Default: 2
MIN_COLS_FOR_TABLE=2
