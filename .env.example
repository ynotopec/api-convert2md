# Required: must match OpenWebUI's EXTERNAL_DOCUMENT_LOADER_API_KEY
ENGINE_API_KEY=supersecret

# PDF parsing scope ("all" or page ranges like "1-5")
PDF_PAGES=all

# Chunking / emission
MAX_DOC_CHARS=6000
OVERLAP_CHARS=800
MAX_TEXT_PAGES=200

# Multi-header reconstruction
MAX_HEADER_ROWS=4

# Camelot tuning
CAMELOT_LATTICE_LINE_SCALE=40
CAMELOT_STREAM_EDGE_TOL=200
CAMELOT_STREAM_ROW_TOL=10

# Table quality heuristics
MIN_ROWS_FOR_TABLE=2
MIN_COLS_FOR_TABLE=2
